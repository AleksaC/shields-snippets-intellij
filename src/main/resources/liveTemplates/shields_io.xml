            <option name="MARKDOWN" value="true"/>
        </context>
    </template>
    <template
        name="sioisc"
        value="[![License: ISC](https://img.shields.io/badge/License-ISC-blue.svg)]($URL$)"
        toReformat="false"
        toShortenFQNames="true"
    >
