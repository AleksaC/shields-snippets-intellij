<templateSet group="shields.io">
    <!-- Licenses -->
    <template
        name="siomit"
        value="[![license](https://img.shields.io/badge/License-MIT-green.svg)]($URL$)"
        toReformat="false"
        toShortenFQNames="true"
    >
        <variable name="URL" expression="" defaultValue="&quot;LICENSE&quot;" alwaysStopAt="true"/>
        <context>
            <option name="MARKDOWN" value="true"/>
        </context>
    </template>
