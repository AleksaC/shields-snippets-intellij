<templateSet group="shields.io">
    <!-- Licenses -->
    <template
        name="siomit"
        value="[![license](https://img.shields.io/badge/License-MIT-green.svg)]($URL$)"
        toReformat="false"
        toShortenFQNames="true"
    >
        <variable name="URL" expression="" defaultValue="&quot;LICENSE&quot;" alwaysStopAt="true"/>
        <context>
            <option name="MARKDOWN" value="true"/>
        </context>
    </template>
    <template
        name="sioapc"
        value="[![License](https://img.shields.io/badge/License-Apache%202.0-blue.svg)]($URL$)"
        toReformat="false"
        toShortenFQNames="true"
    >
        <variable name="URL" expression="" defaultValue="&quot;LICENSE&quot;" alwaysStopAt="true"/>
        <context>
            <option name="MARKDOWN" value="true"/>
        </context>
    </template>
    <template
        name="siobsd3"
        value="[![License](https://img.shields.io/badge/License-BSD%203--Clause-blue.svg)]($URL$)"
        toReformat="false"
        toShortenFQNames="true"
    >
        <variable name="URL" expression="" defaultValue="&quot;LICENSE&quot;" alwaysStopAt="true"/>
        <context>
            <option name="MARKDOWN" value="true"/>
        </context>
    </template>
    <template
        name="siobsd2"
        value="[![License](https://img.shields.io/badge/License-BSD%202--Clause-orange.svg)]($URL$)"
